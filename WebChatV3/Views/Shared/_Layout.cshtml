        }

        .item-2hkk8m {
            height: 56px !important;
            width: 48px;
            margin: 0;
            border-radius: 0;
            transform: unset !important;
            clip-path: inset(0 round 20%);
            transition: .2s linear;
            opacity: 1 !important;
            background: rgba(80, 80, 80, 0);
        }

            .item-2hkk8m[style*="opacity: 0.7"] {
                background: rgba(200, 200, 200, .6)
            }

            .item-2hkk8m[style*="opacity: 1"] {
                background: rgba(200, 200, 200, .8)
            }

        .test2 {
            position: relative;
            display: -webkit-box;
            display: -ms-flexbox;
            display: flex;
            -webkit-box-pack: center;
            -ms-flex-pack: center;
            justify-content: center;
            width: 48px;
            outline: none;
        }

        .logodc {
            height: 54px;
            margin-bottom: 8px;
        }

        .cotent {
            flex-grow: 100;
        }
    </style>
